<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.jamesgiang.quakefelt"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="4" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-library android:name="com.google.android.maps" />
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@style/Default">
        <activity android:name=".Launch" android:label="@string/app_name" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity android:name=".Form"  android:label="@string/app_name" android:screenOrientation="portrait"/>
		<activity android:name=".MainMap"  android:label="@string/app_name" android:screenOrientation="portrait"/>
		<activity android:name=".MainList"  android:label="@string/app_name" android:screenOrientation="portrait"/>
		<activity android:name=".QuakeSelect"  android:label="@string/app_name" android:screenOrientation="portrait"/>
		<activity android:name=".Finish"  android:label="@string/app_name" android:screenOrientation="portrait"/>
    </application>
</manifest>